    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@vueuse/core": "^10.7.1",
    "@vueuse/router": "^10.7.1",
    "apache-arrow": "14.0.2",
    "axios": "^1.6.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "d3-array": "^3.2.2",
    "d3-color": "^3.1.0",
    "d3-contour": "^4.0.2",
    "d3-ease": "^3.0.1",
    "d3-fetch": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-geo": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-random": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-timer": "^3.0.1",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "glsl-easings": "^1.0.0",
    "glsl-fast-gaussian-blur": "^1.0.2",
    "glsl-read-float": "^1.1.0",
    "lodash.merge": "^4.6.2",
    "pako": "^2.1.0",
    "pinia": "^2.1.7",
