{
  "name": "topic-explorer",
  "version": "0.0.0",
  "type": "module",
  "config": {
    "backend": "http://127.0.0.1:8082"
  },
  "scripts": {
    "dev": "vite --host 127.0.0.1 --mode dev",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "api": "node ./tools/openapi-gen/bin/index.js --input $npm_package_config_backend/openapi.json --output src/plugins/api/api-backend --name BackendClient --client axios --indent 2 --useOptions --exportModels true --exportServices true --exportSchemas true --exportCore false --corePathTop @/plugins/api/core --corePathDeep @/plugins/api/core"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.28.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
