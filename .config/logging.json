{
  "version" : 1,
  "disable_existing_loggers" : false,
  "root" : {
    "level" : "DEBUG",
    "handlers" : [
      "default"
    ]
  },
  "formatters" : {
    "default" : {
      "()" : "server.logging.ColourFormatter",
      "fmt" : "%(asctime)s [%(levelnamec)s] %(name)s: %(message)s",
      "use_colors" : true
    },
    "process" : {
      "format" : "%(asctime)s [%(levelname)s] %(processName)-10s %(name)s: %(message)s"
    },
    "access" : {
      "format" : "%(asctime)s [   REQ  ] %(name)s: %(message)s"
    }
  },
  "handlers" : {
    "default" : {
      "level" : "DEBUG",
      "formatter" : "default",
      "class" : "logging.StreamHandler",
      "stream" : "ext://sys.stdout"
    },
    "access" : {
      "level" : "DEBUG",
      "formatter" : "access",
      "class" : "logging.StreamHandler",
      "stream" : "ext://sys.stdout"
    }
  },
  "loggers" : {
    "hypercorn.access" : {
      "handlers" : [
        "access"
      ],
      "level" : "DEBUG",
      "propagate" : false
    },
    "hypercorn.error" : {
      "handlers" : [
        "default"
      ],
      "level" : "DEBUG"
    },
    "aiosqlite" : {
      "level" : "ERROR"
    },
    "databases" : {
      "level" : "ERROR"
    },
    "sqlite" : {
      "level" : "DEBUG"
    }
  }
}